{
  "name": "my-11ty-starter",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "cross-env ELEVENTY_ENV=production run-s clean build:parcel hash build:eleventy",
    "build:parcel": "parcel build",
    "build:eleventy": "eleventy",
    "clean": "rimraf _site dist",
    "debug": "cross-env DEBUG-* yarn build",
    "dev": "cross-env ELEVENTY_ENV=development run-s unhash watch",
    "hash": "node hashCssJs.js",
    "unhash": "node unHashCssJs.js",
    "lint": "eslint",
    "start": "yarn dev",
    "watch": "run-p watch:*",
    "watch:bs": "browser-sync start --config .bsconfig.js",
    "watch:eleventy": "eleventy --watch",
    "watch:parcel": "parcel watch"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@parcel/transformer-sass": "2.4.1",
    "browser-sync": "^2.27.9",
    "cross-env": "^7.0.3",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "html-minifier": "^4.0.0",
    "md5": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.4.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2"
  },
  "browserslist": "defaults",
  "targets": {
    "css": {
      "source": "./src/css/index.scss",
      "distDir": "./dist/css"
    },
    "js": {
      "source": "./src/js/index.js",
      "distDir": "./dist/js"
    }
  }
}
